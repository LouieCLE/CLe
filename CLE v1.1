-- Un-type and Type Text
local function typeAndUntype(obj, texts)
while true do
for _, text in ipairs(texts) do
-- Type
for i = 1, #text do
obj.Text = string.sub(text, 1, i)
task.wait(0.05)
end
task.wait(1.5)
-- Untype
for i = #text, 0, -1 do
obj.Text = string.sub(text, 1, i)
task.wait(0.04)
end
task.wait(0.5)
end
end
end

