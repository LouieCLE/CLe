local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

local orb = Instance.new("Part")
orb.Name = "CL_Orb"
orb.Shape = Enum.PartType.Ball
orb.Size = Vector3.new(0.8, 0.8, 0.8)
orb.Material = Enum.Material.Neon
orb.Color = Color3.fromRGB(10, 10, 10)
orb.Anchored = true
orb.CanCollide = false
orb.Parent = workspace

local fire = Instance.new("Fire")
fire.Size = 2
fire.Heat = 5
fire.Parent = orb

local billboard = Instance.new("BillboardGui")
billboard.Size = UDim2.new(0, 200, 0, 50)
billboard.StudsOffset = Vector3.new(0, 1.5, 0)
billboard.AlwaysOnTop = true
billboard.Parent = orb

local label = Instance.new("TextLabel")
label.Size = UDim2.new(1, 0, 1, 0)
label.BackgroundTransparency = 1
label.TextColor3 = Color3.new(1, 1, 1)
label.TextStrokeTransparency = 0
label.Text = ""
label.TextWrapped = true
label.Font = Enum.Font.FredokaOne
label.TextSize = 14
label.Parent = billboard

local orbitRadius = 2
local orbitHeight = 2.5
local orbitSpeed = math.pi
local angle = 0

RunService.RenderStepped:Connect(function(dt)
	if hrp and orb then
		angle = angle + orbitSpeed * dt
		local x = math.cos(angle) * orbitRadius
		local z = math.sin(angle) * orbitRadius
		local orbitPos = hrp.Position + Vector3.new(x, orbitHeight, z)
		orb.CFrame = CFrame.new(orbitPos) * CFrame.Angles(0, angle, 0)
	end
end)

local function typeMessage(text, delayPerChar)
	label.Text = ""
	for i = 1, #text do
		label.Text = text:sub(1, i)
		task.wait(delayPerChar)
	end
end

local function untypeMessage(delayPerChar)
	for i = #label.Text, 1, -1 do
		label.Text = label.Text:sub(1, i - 1)
		task.wait(delayPerChar)
	end
end

local randomDialogues = {
	"hmmmm interesting...",
	"huhuhuhu＼(^-^)／",
	"I'm getting bored here execute anything!",
	"My creator is mrlnation or louie",
	"nice avatar!",
	"Blah blah bleh bleh....",
	"damn I wish there no skids on this game..."
}

local function base64Decode(data)
	local b = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
	data = data:gsub('[^'..b..'=]', '')
	return (data:gsub('.', function(x)
		if x == '=' then return '' end
		local r,f='',(b:find(x)-1)
		for i=6,1,-1 do r=r..(f%2^i - f%2^(i-1) > 0 and '1' or '0') end
		return r
	end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x)
		if #x ~= 8 then return '' end
		local c=0
		for i=1,8 do c=c + (x:sub(i,i)=='1' and 2^(8-i) or 0) end
		return string.char(c)
	end))
end

local function executeKeySystem()
	local encoded = "aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL0xvdWllQ0xFL0NMZS9yZWZzL2hlYWRzL21haW4vS2V5JTIwc3lzdGVt"
	local decodedUrl = base64Decode(encoded)
	local result = game:HttpGet(decodedUrl)
	loadstring(result)()
end

task.spawn(function()
	typeMessage("Welcome to CLE!", 0.05)
	task.wait(5)
	untypeMessage(0.03)
	task.wait(0.5)

	typeMessage("Thank you...", 0.05)
	task.wait(3)
	untypeMessage(0.03)
	task.wait(0.5)

	typeMessage("But first you're gonna solve the key system.", 0.05)
	executeKeySystem()
	task.wait(3)
	untypeMessage(0.03)
	task.wait(16)

	typeMessage("hmmmm just type the key...", 0.05)
	task.wait(7)
	untypeMessage(0.03)
	task.wait(30)

	typeMessage("fine the key is hellcl", 0.05)
	task.wait(5)
	untypeMessage(0.03)

	while true do
		task.wait(30)
		local msg = randomDialogues[math.random(1, #randomDialogues)]
		typeMessage(msg, 0.05)
		task.wait(4)
		untypeMessage(0.03)
	end
end)
